AIDJOBS_ENV=dev
SUPABASE_URL=
SUPABASE_DB_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_KEY=
DATABASE_URL=
MEILI_HOST=
MEILI_MASTER_KEY=
MEILI_JOBS_INDEX=jobs_index
OPENROUTER_API_KEY=
OPENROUTER_MODEL=anthropic/claude-3-haiku
CV_MAX_FILE_MB=6
CV_TRANSIENT_HOURS=48
PAYMENT_PROVIDER=auto
PAYPAL_CLIENT_ID=
PAYPAL_CLIENT_SECRET=
PAYPAL_MODE=sandbox
RAZORPAY_KEY_ID=
RAZORPAY_KEY_SECRET=

# Admin password for /api/admin/login authentication
# Required in production (generate with: openssl rand -hex 32)
ADMIN_PASSWORD=

# Cookie signing secret for httpOnly session cookies (8h expiry)
# Required for admin authentication (generate with: openssl rand -hex 32)
COOKIE_SECRET=

# Crawler configuration
AIDJOBS_DISABLE_SCHEDULER=false
AIDJOBS_CRAWLER_UA=AidJobsBot/1.0 (+contact@aidjobs.app)
AIDJOBS_CONTACT_EMAIL=contact@aidjobs.app

# Feature flags
AIDJOBS_ENABLE_SEARCH=true
AIDJOBS_ENABLE_CV=false
AIDJOBS_ENABLE_FINDEARN=true
AIDJOBS_ENABLE_PAYMENTS=false

# Frontend environment
NEXT_PUBLIC_AIDJOBS_ENV=dev
NEXT_PUBLIC_MEILI_FALLBACK=true

# Pipeline storage integration (optional)
# Generate with: python -c "import secrets; print(secrets.token_urlsafe(32))"
# Or: openssl rand -hex 32
EXTRACTION_USE_STORAGE=false
EXTRACTION_SHADOW_MODE=true
INTERNAL_API_KEY=
